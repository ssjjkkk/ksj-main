<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="user">
	<select id="getUserNick" parameterClass="String" resultClass="String">
		SELECT nick FROM t_user WHERE nick = #nick#
	</select>
	
	<insert id="insertUser" parameterClass="userVO">
		INSERT INTO t_user (user_no, email, pass, nick, name, bir, gender)
		VALUES (user_no.NEXTVAL, #email#, #pass#, #nick#, #name#, #bir#, #gender#)
	</insert>
	

</sqlMap>